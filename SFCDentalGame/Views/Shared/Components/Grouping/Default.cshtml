<table class="table table-hover">
  <thead>
    <tr bgcolor="#96C02F">
      <th>Badge</th>
      <th>Type Grouping</th>
      <th>Description</th>
    </tr>
     </thead>
    <tbody>
        @foreach(IGrouping<string, string> top in ViewBag.gr)
        {
        <tr>
            <td></td>
            <td>@top.Key</td>
            <td>
            @foreach(string behavs in top){
                <tr>
                    <td>@(behavs)</td>
                </tr>
            }
          </td>
        </tr>
        }
    </tbody>
    </table>